<p id="notice"><%= notice %></p>

<h1>Agencia</h1>

<table>
  <thead>
    <tr>
      <th>Agencia</th>
      <th>Numero conta</th>
      <th>Limite</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agencia.each do |agencium| %>
      <tr>
        <td><%= agencium.agencia_id %></td>
        <td><%= agencium.numero_conta %></td>
        <td><%= agencium.limite %></td>
        <td><%= link_to 'Show', agencium %></td>
        <td><%= link_to 'Edit', edit_agencium_path(agencium) %></td>
        <td><%= link_to 'Destroy', agencium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agencium', new_agencium_path %>
